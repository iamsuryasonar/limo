{
    "name": "Tarchive",
    "description": "archive tabs",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "pop_up/pop_up.html",
        "default_icon": "images/logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "scripts/content.js"
            ]
        }
    ],
    "background": {
        "service_worker": "scripts/background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "tabs",
        "storage"
    ]
}