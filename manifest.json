{
    "name": "buckets of tab",
    "description": "store tabs in buckets",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "pop_up/pop_up.html",
        "default_icon": "images/logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "scripts/content.js"
            ]
        }
    ],
    "background": {
        "service_worker": "scripts/background.js"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "permissions": [
        "tabs",
        "storage"
    ]
}